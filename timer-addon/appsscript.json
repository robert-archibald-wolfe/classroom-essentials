        "primaryColor": "#1967d2"
      },
      "homepageTrigger": {
        "runFunction": "showTimerSidebar"
      }
    },
    "slides": {
      "onFileScopeGrantedTrigger": {
        "runFunction": "onOpen"
      }
    }
  }
}
